{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "mochawesome-merge": "bin/mochawesome-merge.js"
  },
  "scripts": {
    "dev": "next -p 8080",
    "build": "next build",
    "start": "next start",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "lint": "npm run lint:scss && npm run lint:js",
    "lint:scss": "stylelint 'app/**/*.scss' --syntax scss --fix ; exit 0",
    "lint:js": "eslint . --ext .tsx",
    "test:unit": "jest --passWithNoTests",
    "test:unit:update": "jest --passWithNoTests --updateSnapshot",
    "type:check": "tsc",
    "delete:reports": "rm test/results || true",
    "prereport": "npm run delete:reports",
    "report": "cypress run --record true",
    "report:merge": "mochawesome-merge --reportDir test/results > mochawesome.json",
    "report:generate": "marge mochawesome.json",
    "postreport": "npm run report:merge && npm run report:generate"
  },
  "dependencies": {
    "antd": "^4.14.0",
    "axios": "^0.21.1",
    "next": "10.0.9",
    "next-i18next": "^8.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.1",
    "@testing-library/dom": "^7.26.3",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.12.50",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-jest": "^26.6.1",
    "cypress": "^6.8.0",
    "cypress-multi-reporters": "^1.4.0",
    "eslint": "^7.22.0",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.1",
    "jest-dom": "^4.0.0",
    "lint-staged": "^10.5.0",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "prettier": "2.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.3"
  }
}
